@using VersionsCRUD.Environment
@model EnvironmentGetResp
<link rel="stylesheet" href="@Url.Content("~/assets/css/site.css")" type="text/css" />

<table id="example" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Environment Name</th>
            <th>Description</th>
            @*<th>Project ID</th>*@
            <th>Project Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var environment in Model.environments)
        {
            <tr>
                <td>@environment.name</td>
                <td>@environment.description</td>
                @*<td>@environment.projectid</td>*@                
                <td>@environment.projectName</td>
            </tr>
        }
    </tbody>
</table>

@section scripts {
    <script>
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
}