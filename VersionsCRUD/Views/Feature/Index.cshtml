@using VersionsCRUD.Feature
@model FeatureGetResp
<link rel="stylesheet" href="@Url.Content("~/assets/css/site.css")" type="text/css" />

<table id="example" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Release</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var feature in Model.features)
        {
            <tr>
                <td>@feature.name</td>
                <td>@feature.description</td>
                <td>@feature.release</td>
            </tr>
        }
    </tbody>
</table>
