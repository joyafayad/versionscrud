@using VersionsCRUD.User
@model UserGetResp
<link rel="stylesheet" href="@Url.Content("~/assets/css/site.css")" type="text/css" />

<table id="example" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.users)
        {
            <tr>
                <td>@user.username</td>
                <td>@user.email</td>
                <td>@user.password</td>
            </tr>
        }
    </tbody>
</table>

