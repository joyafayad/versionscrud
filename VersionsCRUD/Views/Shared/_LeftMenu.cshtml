<button class="sign-out-button" onclick="LogoutF()">Sign Out</button>


<script>
    function LogoutF(){
		$.ajax({
			url: '/Login/Logout',
			type: 'POST',
			contentType: 'application/json',
			success: function (result) {
				location.reload();
			},
			error: function (xhr, status, error) {
				console.error(xhr.responseText);
			}
		});
    }
</script>
